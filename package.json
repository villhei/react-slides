{
  "name": "react-slides",
  "version": "1.0.0",
  "description": "React slideshow library",
  "scripts": {
    "postinstall": "cp scripts/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit && echo 'Git hooks set'",
    "start:dev": "yarn parcel examples/basic/index.html",
